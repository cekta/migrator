{
    "name": "cekta/migrator",
    "description": "migration tool",
    "type": "library",
    "keywords": [
        "migration",
        "migrations",
        "migrator"
    ],
    "license": "GPL-3.0-only",
    "require": {
        "symfony/console": "@stable",
        "php": ">=8.1",
        "ext-pdo": "*",
        "psr/container": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Cekta\\Migrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cekta\\Migrator\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "@stable",
        "phpunit/phpunit": "@stable",
        "phpstan/phpstan": "@stable"
    }
}
